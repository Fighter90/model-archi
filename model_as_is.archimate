<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:archimate="http://www.archimatetool.com/archimate"
                 name="AS-IS Fitness Club Business Model"
                 id="id-fitness-asis-2025"
                 version="4.6.0">

  <folder name="Strategy" id="id-strategy-folder" type="strategy"/>

  <folder name="Business" id="id-business-folder" type="business">
    <element xsi:type="archimate:BusinessActor" name="Клиент" id="actor-client">
      <documentation>Физическое лицо, пользующееся услугами фитнес-клуба</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessActor" name="Администратор" id="actor-admin">
      <documentation>Сотрудник стойки администратора, ведущий запись и коммуникацию</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessActor" name="Тренер" id="actor-trainer">
      <documentation>Сотрудник, проводящий занятия</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessActor" name="Руководство клуба" id="actor-management">
      <documentation>Менеджмент, принимающий управленческие решения</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessActor" name="Бухгалтер" id="actor-accountant">
      <documentation>Сотрудник, обрабатывающий платежи и финансовую отчётность</documentation>
      <property key="Area" value="Отчётность"/>
    </element>

    <element xsi:type="archimate:BusinessRole" name="Роль: Покупатель услуг" id="role-client">
      <documentation>Роль клиента при взаимодействии с клубом</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessRole" name="Роль: Front Desk" id="role-frontdesk">
      <documentation>Роль администратора на стойке регистрации</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessRole" name="Роль: Тренер" id="role-trainer">
      <documentation>Профессиональная роль тренера</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessRole" name="Роль: Менеджмент" id="role-management">
      <documentation>Управленческая роль</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessRole" name="Роль: Финансист" id="role-accountant">
      <documentation>Роль ответственного за финансы</documentation>
      <property key="Area" value="Отчётность"/>
    </element>

    <element xsi:type="archimate:BusinessInterface" name="Стойка администратора" id="iface-desk">
      <documentation>Физическая стойка для взаимодействия с клиентами</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessInterface" name="Телефонная линия" id="iface-phone">
      <documentation>Канал телефонной связи с клиентами</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessInterface" name="Доска объявлений" id="iface-board">
      <documentation>Информационная доска в зоне ресепшена</documentation>
      <property key="Area" value="Запись"/>
    </element>

    <element xsi:type="archimate:BusinessService" name="Сервис: Запись на тренировку" id="svc-booking">
      <documentation>Услуга записи клиента на занятие</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Ведение расписания" id="svc-schedule">
      <documentation>Услуга управления расписанием занятий</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Ведение карточек клиентов" id="svc-card-mgmt">
      <documentation>Услуга управления данными клиентов</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Учёт посещаемости" id="svc-attendance">
      <documentation>Услуга отметки и учёта посещений</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Продажа абонементов" id="svc-sales">
      <documentation>Услуга продажи и оформления абонементов</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Уведомления" id="svc-notify">
      <documentation>Услуга информирования клиентов</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Управленческая отчётность" id="svc-reporting">
      <documentation>Услуга формирования отчётов для руководства</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessService" name="Сервис: Обработка платежей" id="svc-payment">
      <documentation>Услуга приёма и учёта платежей</documentation>
      <property key="Area" value="Отчётность"/>
    </element>

    <element xsi:type="archimate:BusinessFunction" name="Функция: Планирование занятий" id="func-planning">
      <documentation>Общая функция планирования тренировочного расписания</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessFunction" name="Функция: Ведение клиентских записей" id="func-records">
      <documentation>Общая функция управления данными клиентов</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessFunction" name="Функция: Продажи и платежи" id="func-sales">
      <documentation>Общая функция коммерческих операций</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessFunction" name="Функция: Сбор и анализ показателей" id="func-analytics">
      <documentation>Общая функция аналитики и отчётности</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessFunction" name="Функция: Коммуникация с клиентами" id="func-communication">
      <documentation>Общая функция взаимодействия и информирования</documentation>
      <property key="Area" value="Запись"/>
    </element>

    <element xsi:type="archimate:BusinessProcess" name="Процесс: Запись клиента" id="proc-client-booking">
      <documentation>Первичный приём запроса на запись</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Проверка доступных слотов" id="proc-slot-check">
      <documentation>Проверка свободных мест в расписании</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Создание записи/брони" id="proc-reserve">
      <documentation>Фиксация брони в журнале</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Предложение альтернатив" id="proc-offer-alt">
      <documentation>Поиск и предложение других вариантов времени</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Сбор расписаний тренеров" id="proc-trainer-schedule">
      <documentation>Получение графиков от тренеров</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Ведение общего расписания" id="proc-main-schedule">
      <documentation>Формирование единого расписания клуба</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Внесение изменений" id="proc-change">
      <documentation>Корректировка расписания при изменениях</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Разрешение накладок" id="proc-resolve-conflict">
      <documentation>Устранение конфликтов в расписании</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Продажа абонемента" id="proc-sell-subscription">
      <documentation>Оформление покупки абонемента</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Запись оплаты/выдача квитанции" id="proc-receipt">
      <documentation>Фиксация платежа и выдача документа</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Ручное уведомление/звонок" id="proc-manual-notify">
      <documentation>Телефонное информирование клиента</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Регистрация клиента" id="proc-register">
      <documentation>Создание новой карточки клиента</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Поиск карточки" id="proc-find-card">
      <documentation>Поиск существующей карточки в архиве</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Отметка посещения" id="proc-check-in">
      <documentation>Фиксация факта посещения занятия</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Формирование отчёта" id="proc-report">
      <documentation>Сбор и обобщение данных для руководства</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Отмена записи" id="proc-cancel-booking">
      <documentation>Отмена ранее созданной брони</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Проверка остатка абонемента" id="proc-check-balance">
      <documentation>Проверка количества оставшихся занятий</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Архивирование карточек" id="proc-archive">
      <documentation>Перемещение неактивных карточек в архив</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessProcess" name="Процесс: Сверка платежей" id="proc-reconcile">
      <documentation>Сопоставление платежей с записями продаж</documentation>
      <property key="Area" value="Отчётность"/>
    </element>

    <element xsi:type="archimate:BusinessObject" name="Объект: Бумажное расписание" id="obj-paper-schedule">
      <documentation>Журнал или листы с текущим расписанием</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Карточка клиента" id="obj-client-card">
      <documentation>Бумажная форма с данными клиента</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Журнал посещаемости" id="obj-attendance-log">
      <documentation>Документ для отметок посещений</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Журнал продаж" id="obj-sales-log">
      <documentation>Ведомость продаж абонементов</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Заметки тренеров" id="obj-trainer-notes">
      <documentation>Записи графиков и пожеланий тренеров</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Телефон клиента" id="obj-client-phone">
      <documentation>Контактный номер телефона</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Квитанция" id="obj-receipt">
      <documentation>Документ подтверждения оплаты</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Прайс-лист" id="obj-price-list">
      <documentation>Список тарифов и цен</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Остаток посещений" id="obj-visit-balance">
      <documentation>Количество оставшихся занятий по абонементу</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Журнал инцидентов" id="obj-incident-log">
      <documentation>Фиксация конфликтов и проблем</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Отмена/перенос" id="obj-cancel-move">
      <documentation>Записи об изменениях расписания</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Касса" id="obj-cashbox">
      <documentation>Наличные средства и учёт платежей</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Архив карточек" id="obj-card-archive">
      <documentation>Хранилище неактивных карточек клиентов</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessObject" name="Объект: Список группы" id="obj-group-list">
      <documentation>Список участников группового занятия</documentation>
      <property key="Area" value="Запись"/>
    </element>

    <element xsi:type="archimate:Contract" name="Контракт: Условия абонемента" id="contract-sub">
      <documentation>Договор на оказание услуг фитнес-клуба</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:Product" name="Продукт: Абонементы и занятия" id="product-pass">
      <documentation>Коммерческий продукт клуба</documentation>
      <property key="Area" value="Запись"/>
    </element>

    <element xsi:type="archimate:BusinessEvent" name="Событие: Клиент звонит/приходит" id="event-client-comes">
      <documentation>Инициация процесса записи</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Время свободно/занято" id="event-slot-free-busy">
      <documentation>Результат проверки доступности слота</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Изменение расписания" id="event-schedule-change">
      <documentation>Факт корректировки графика</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Занятие отменено" id="event-class-cancel">
      <documentation>Отмена тренировки</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Клиент прибыл" id="event-client-arrive">
      <documentation>Клиент пришёл на занятие</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Двойное бронирование" id="event-double-book">
      <documentation>Конфликт в расписании</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: No-show клиента" id="event-no-show">
      <documentation>Клиент не явился на занятие</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Истечение абонемента" id="event-abn-expiry">
      <documentation>Окончание срока действия абонемента</documentation>
      <property key="Area" value="Хранение"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Запрос на отчёт" id="event-report-request">
      <documentation>Обращение руководства за данными</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Отчёт сформирован" id="event-report-gen">
      <documentation>Завершение формирования отчёта</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Запись подтверждена" id="event-booking-confirmed">
      <documentation>Бронь успешно создана</documentation>
      <property key="Area" value="Запись"/>
    </element>
    <element xsi:type="archimate:BusinessEvent" name="Событие: Оплата получена" id="event-payment-received">
      <documentation>Платёж зарегистрирован</documentation>
      <property key="Area" value="Отчётность"/>
    </element>
  </folder>

  <folder name="Application" id="id-application-folder" type="application"/>
  <folder name="Technology &amp; Physical" id="id-technology-folder" type="technology"/>
  <folder name="Motivation" id="id-motivation-folder" type="motivation"/>
  <folder name="Implementation &amp; Migration" id="id-implementation-folder" type="implementation_migration"/>
  <folder name="Other" id="id-other-folder" type="other">
    <element xsi:type="archimate:Grouping" name="Область: Запись (Booking)" id="grp-booking">
      <documentation>Процессы записи клиентов на тренировки, ведение расписания, продажа абонементов</documentation>
    </element>
    <element xsi:type="archimate:Grouping" name="Область: Хранение данных (Storage)" id="grp-storage">
      <documentation>Процессы ведения карточек клиентов, учёт посещаемости, хранение данных</documentation>
    </element>
    <element xsi:type="archimate:Grouping" name="Область: Отчётность (Reporting)" id="grp-reporting">
      <documentation>Формирование управленческой отчётности и аналитики</documentation>
    </element>
  </folder>

